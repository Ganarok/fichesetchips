{
    "name": "backexpressesp",
    "version": "1.0.0",
    "description": "",
    "main": "server.ts",
    "scripts": {
        "test": "mocha --exit --timeout 10000 -r ts-node/register src/test/hook.ts",
        "dev": "ts-node-dev --respawn --transpile-only ./src/server.ts",
        "start": "ts-node src/server.ts",
        "typeorm": "typeorm-ts-node-esm",
        "migration:generate": "npm run -- typeorm migration:generate --dataSource src/database/init/datasources/public-data-source.ts -p true",
        "migration:run": "npm run -- typeorm migration:run --dataSource src/database/init/datasources/public-data-source.ts",
        "migration:create": "npm run -- typeorm migration:create",
        "migration:revert": "npm run -- typeorm migration:revert --dataSource src/database/init/datasources/public-data-source.ts",
        "migration:show": "npm run -- typeorm migration:show --dataSource src/database/init/datasources/public-data-source.ts",
        "db:seed": "cross-env DB_MIGRATION_DIR=src/database/seeders/public/*.ts npm run -- typeorm migration:run --dataSource src/database/init/datasources/public-data-source.ts && cross-env DB_MIGRATION_DIR=src/database/seeders/characters/*.ts npm run -- typeorm migration:run --dataSource src/database/init/datasources/public-data-source.ts",
        "db:refresh": "npm run -- typeorm schema:drop --dataSource src/database/init/datasources/public-data-source.ts && npm run migration:run && npm run db:seed",
        "db:create": "npm run -- typeorm query $1 --dataSource src/database/init/datasources/empty-data-source.ts"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/chai": "^4.3.3",
        "@types/chai-http": "^4.2.0",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.14",
        "@types/jsonwebtoken": "^8.5.9",
        "@types/mocha": "^10.0.0",
        "@types/node": "^18.7.23",
        "@types/swagger-jsdoc": "^6.0.1",
        "@types/swagger-ui-express": "^4.1.3",
        "@typescript-eslint/eslint-plugin": "^5.38.1",
        "@typescript-eslint/parser": "^5.38.1",
        "chai": "^4.3.6",
        "chai-http": "^4.3.0",
        "concurrently": "^7.4.0",
        "cross-env": "^7.0.3",
        "dotenv": "^16.0.3",
        "eslint": "^8.24.0",
        "mocha": "^10.1.0",
        "ts-node": "^10.9.1",
        "ts-node-dev": "^2.0.0",
        "typescript": "^4.8.4"
    },
    "dependencies": {
        "@types/supertest": "^2.0.12",
        "bcrypt": "^5.0.1",
        "body-parser": "^1.20.1",
        "cors": "^2.8.5",
        "cross-env": "^7.0.3",
        "express": "^4.18.1",
        "fs": "^0.0.1-security",
        "jsonwebtoken": "^8.5.1",
        "pg": "^8.8.0",
        "reflect-metadata": "^0.1.13",
        "socket.io": "^4.5.4",
        "sqlite3": "^5.1.1",
        "supertest": "^6.3.0",
        "swagger-jsdoc": "6.2.5",
        "swagger-ui-express": "^4.5.0",
        "tsoa": "^4.1.3",
        "typeorm": "^0.3.10"
    }
}