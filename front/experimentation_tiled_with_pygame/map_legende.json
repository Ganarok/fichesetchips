{
    "height": 40,                               // Nombre de tile (verticaux) de la totalité de la map
    "layers": [{                                // Plusieurs layers possible (pour mettre des datas en 1er plan ou en arrière plan)
        "data": ["xxx"],                        // Données du layer
        "height": 40,                           // nombre de tile (vertical) du layer
        "name": "Ground",                       // Nom du layer
        "opacity": 1,                           // Opacité de chacune des tiles du layer
        "type": "tilelayer",                    // Type du layer : tilelayer, objectgroup, imagelayer or group
        "visible": true,                        // Booléen de la visualité du layer
        "width": 40,                            // nombre de tiles (horizontal) du layer
        "x": 0,                                 // origine horizontale du layer
        "y": 0                                  // origine verticale du layer
    }],
    "orientation": "orthogonal",                // visualisation
    "properties": [{                            //Tableau de propriété
        "name": "string",                       //Nom de la property
        "type": "string",                       // Type de la propriété --> (string (default), int, float, bool, color, file, object or class (since 0.16, with color and file added in 0.17, object added in 1.4 and class added in 1.8))
        "propertytype": "string",               // custom property type --> https://doc.mapeditor.org/en/stable/manual/custom-properties/#custom-property-types
        "value": "depending on propertytype"    //Value of the property
    }],
    "tileheight": 32,                           // nombre de pixels verticaux de chaque tile
    "tilesets": [{                              // Renseignements des tileset (plusieurs possible)
        "firstgid": 1,                          // ???
        "image": "path",                        // Path vers le tileset
        "imageheight": 199,                     // taille totale du tileset (vertical)
        "imagewidth": 265,                      // taille totale du tileset (horizontal)
        "margin": 1,                            // Marge avant, après, au dessus et en dessous du tileset
        "name": "Desert",                       // Nom du tileset
        "properties": {                         // ???
        },
        "spacing": 1,                           // Nombre de pixels separants les tiles
        "tileheight": 32,                       // Taille d'un tile (vertical) pour le tileset
        "tilewidth": 32                         // Taille d'un tile (horizontal) pour le tileset
    }],
    "tilewidth": 32,                            // nombre de pixels horizontaux de chaque tile
    "version": 1,                               // Version de la tilemap
    "width": 40                                 // Nombre de tile (horizontaux) de la totalité de la map
}

// Théorie : layer[x] = tileset[x] = properties[x], ce qui permet de link la data au tileset et à la property